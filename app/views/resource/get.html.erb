<h1><a href="<%= @headline_url %>"><%= @headline %></a></h1>
<p>Ein Subjekt vom Typ <a href="<%= @type_url %>"><%= @type %></a>, aus dem Bestand von : http://101companies.org</p>
<%= if(@abstract != nil)
      raw '<h2>Abstract</h2><p>' + @abstract.to_s + '</p>'
    end
%>
<style>
  table.res {
    width: 100%;
  }
  tr.o td {
    background-color: #f9f9f8;
  }
  tr.res td {
    border-top: 1px solid #c3d9ff;
    padding: 10px;
  }
</style>
<h2>Eigenschaften</h2>
<table class="res">
  <tr>
    <th style="width: 30%;">Eigenschaft</th>
    <th>Wert</th>
  </tr>

  <% @result.each do |solution| %>
      <tr class="res <%= cycle('o', 'e') %>">
        <td><a href="<%= solution.p %>"><%= solution.p.to_s.split('/').last %></a></td>
        <td><a href="<%= solution.o %>"><%= solution.o.to_s.split('/').last %></a></td>
      </tr>
  <% end %>
  <% @result_inv.each do |solution| %>
      <tr class="res <%= cycle('o', 'e') %>">
        <td>is <a href="<%= solution.p %>"><%= solution.p.to_s.split('/').last %></a> of</td>
        <td><a href="<%= solution.s %>"><%= solution.s.to_s.split('/').last %></a></td>
      </tr>
  <% end %>
</table>