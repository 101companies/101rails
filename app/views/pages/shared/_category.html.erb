<script type="text/javascript">
Backbone.View.prototype._super = function(funcName){
    return this.constructor.__super__[funcName].apply(this, _.rest(arguments));
}
Wiki.page = new Wiki.Models.Category({title: <%= j @page.title.to_json.html_safe -%>});
Wiki.page.set('sections',new Wiki.Models.Sections(<%= j @page.sections.to_json.html_safe -%>));
Wiki.page.set('categories',<%= j @page['categories'].to_json.html_safe -%>);
Wiki.page.set('backlinks',<%= j @page['backlinks'].to_json.html_safe -%>);
Wiki.page.set('members',<%= j @page['members'].to_json.html_safe -%>);
jQuery(document).ready(function ($){
  window.router = new Wiki.Routers.PagesRouter(Wiki.Views.Categories);
  Backbone.history.start();
})

</script>